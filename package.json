{
  "name": "p-activity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "postcss-pxtorem": "^4.0.1",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-pwa": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "js-md5": "^0.7.3",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lib-flexible": "^0.3.2",
    "normalize.css": "^8.0.0",
    "vant": "^1.5.0-beta2",
    "vue-template-compiler": "^2.5.17",
    "vue-wechat-title": "^2.0.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 75,
        "propList": [
          "*"
        ],
        "selectorBlackList": [
          "van-toast",
          "van-picker",
          "van-popup",
          "van-notify"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
